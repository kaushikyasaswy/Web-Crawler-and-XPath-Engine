<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name>Channel Subscription Servlets</display-name>
  <context-param>
    <param-name>BDBstore</param-name>
    <param-value>/home/cis455/workspace/HW2/</param-value>
  </context-param>
  <servlet>
    <servlet-name>xpath</servlet-name>
    <servlet-class>edu.upenn.cis455.servlet.XPathServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>xpath</servlet-name>
    <url-pattern>/xpath</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>handlerservlet</servlet-name>
    <servlet-class>edu.upenn.cis455.servlet.XPathHandlerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>handlerservlet</servlet-name>
    <url-pattern>/handler</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>signup</servlet-name>
  	<servlet-class>edu.upenn.cis455.servlet.SignUpServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>signup</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>login</servlet-name>
  	<servlet-class>edu.upenn.cis455.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>createchannel</servlet-name>
  	<servlet-class>edu.upenn.cis455.servlet.CreateChannelServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>createchannel</servlet-name>
    <url-pattern>/createchannel</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>deletechannel</servlet-name>
  	<servlet-class>edu.upenn.cis455.servlet.DeleteChannelServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>deletechannel</servlet-name>
    <url-pattern>/deletechannel</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>displaychannel</servlet-name>
  	<servlet-class>edu.upenn.cis455.servlet.DisplayChannelServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>displaychannel</servlet-name>
    <url-pattern>/displaychannel</url-pattern>
  </servlet-mapping>
</web-app>